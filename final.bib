Keywords = {Components, Circuits, Devices and Systems, Chaos, Piecewise linear techniques, Differential equations, Eigenvalues and eigenfunctions, Electronic circuits, Resistors, Bifurcation, Laboratories, Operational amplifiers, Diodes},
Number = {11},
